@model IList<MVC_v2.Models.Pulls.Pull>
@{
    ViewData["Title"] = "Home Page";
}

<dif class="row">
    @foreach (var pull in Model)
    {
        <dif class="col-2">@pull.Id</dif>
        <dif class="col-8 text-center border-dark">@pull.CreatorLogin</dif>
        <dif class="col-2">@pull.CreationDate</dif>
    }
</dif>